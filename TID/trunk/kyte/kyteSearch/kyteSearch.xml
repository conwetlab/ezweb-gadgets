<!-- EzWeb Gadgets Tags -->
<Platform.Preferences></Platform.Preferences>
<Platform.StateProperties/>
<!-- EzWeb Gadget Data Wiring -->

<Platform.Wiring>
	<Slot name="keyword" type="text" label="keyword" friendcode="keyword"/>
	<Event name="videoURL" type="text" label="videoURL" friendcode="VideoURL"/>
	<Event name="title" type="text" label="title" friendcode="title"/>
	<Event name="synopsis" type="text" label="synopsis" friendcode="synopsis"/>
	<Event name="tags" type="text" label="tags" friendcode="tags"/>
	<Event name="permalink" type="text" label="permalink" friendcode="url"/>
	<Event name="authorName" type="text" label="authorName" friendcode="authorName"/>
	<Event name="authorURI" type="text" label="authorURI" friendcode="authorURI"/>
</Platform.Wiring>
<Platform.Context>
	<Context name="language" type="text" concept="language"/>
</Platform.Context>
<Platform.Link>
	<XHTML href="http://demo.ezweb.morfeo-project.org/repository/kyte/kyteSearch/kyteSearch.html"/>
</Platform.Link>
<Platform.Rendering width="7" height="26"/>
